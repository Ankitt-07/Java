public class method_2 {


public static void main(String[] args)
{
   A.show();
}

class A {

static void show()
{
    int i , sum = 0;
    for(i = 1; i< 10;i++)
    {
       sum = sum +i;
       System.out.println(i);
      
    }
    System.out.println(" The total value of sum is " +sum);
}

}




    
}
